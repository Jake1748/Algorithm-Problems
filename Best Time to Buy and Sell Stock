class Solution:
    def maxProfit(self, prices: List[int]) -> int:
        if len(prices) == 1:
            return 0

        left = 0
        right = len(prices) - 1
        result = prices[left] - prices[right]

        while left != right:
            print(left, right)
            diff = prices[left] - prices[right]

            if diff > result:
                result = diff

                if price[left] < price[right]:
                    right -= 1
                else:
                    left += 1
            else:
                left += 1

        return result
