class Solution:
    def climbStairs(self, n: int) -> int:
        choices = [0,1] + [ 0 for i in range(n - 1) ]
        
        for step in range(1, 3):
            for i in range( 2, len(choices) ):
                choices[i] = choices[i - 1] + 1 + choices[i - 2]

        print( choices )
        return choices[n]

