# This is a two-pointer system
# O(n) time and O(1) space

class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        slow,fast = 0,1

        while fast < len(nums):
            if nums[slow] == nums[fast]:
                fast += 1
            else:
                nums[slow + 1] = nums[ fast ]
                slow += 1

        return slow + 1

# This solution works for most cases, but causes a timeout error for large lists

class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        if len(nums) == 0:
            return 0
        uniques = set()
        i = 0
        counter = 0
        while True:
            # print('here:', nums)
            num = nums[i]
            if num not in uniques:
                uniques.add(num)
            else:
                for j in range(i, len(nums) - 1 ):
                    temp = nums[j]
                    nums[j] = nums[j + 1]
                    nums[j + 1] = temp
                i -= 1
            i += 1
            counter += 1
            if counter == len(nums):
                break

        return len(uniques) 
